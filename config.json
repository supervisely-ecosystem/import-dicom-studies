{
  "name": "Import dicom studies",
  "type": "app",
  "categories": [
    "Import"
  ],
  "description": "Convert dicom data to nrrd format and creates a new project with grouped images",
  "docker_image": "supervisely/base-py-sdk:6.8.3",
  "main_script": "src/main.py",
  "modal_template": "src/modal.html",
  "modal_template_state": {
    "tagMode": "prepared",
    "groupTag": "StudyInstanceUID",
    "uploadMeta": false,
    "addTagsFromDcm": true,
    "dcmTags": {
      "tags": [
        "Manufacturer",
        "ManufacturerModelName",
        "Modality"
      ]
    }
  },
  "task_location": "workspace_tasks",
  "icon": "https://i.imgur.com/MxlNwDM.png",
  "icon_cover": false,
  "icon_background": "#FFFFFF",
  "headless": true,
  "context_menu": {
    "target": [
      "files_file",
      "files_folder"
    ]
  }
}
